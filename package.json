{
  "name": "eslint-config-appnexus",
  "version": "1.2.0",
  "description": "AppNexus Standard Eslint Config",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint . --ignore-pattern test-*",
    "lint-fix": "eslint . --ignore-pattern test-* --fix",
    "find-unused-rules": "eslint-find-rules -u -n"
  },
  "pre-push": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/appnexus/eslint-config-appnexus"
  },
  "keywords": [
    "AppNexus",
    "config",
    "eslint",
    "eslintconfig",
    "javascript",
    "styleguide"
  ],
  "author": "Tim Santeford <tsanteford@appnexus.com>",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "peerDependencies": {
    "eslint": ">= 3",
    "eslint-plugin-hapi": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-lodash": "^2.1.8",
    "eslint-plugin-promise": "^3.3.1",
    "eslint-plugin-react": "^6.7.1"
  },
  "devDependencies": {
    "eslint": "^3.10.2",
    "eslint-find-rules": "^1.14.3",
    "eslint-plugin-hapi": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-lodash": "^2.1.8",
    "eslint-plugin-promise": "^3.3.1",
    "eslint-plugin-react": "^6.7.1",
    "jest": "^17.0.3",
    "lodash": "^4.17.2",
    "pre-push": "^0.1.1",
    "react": "^15.4.1"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
