{
  "name": "eslint-config-appnexus",
  "version": "2.0.2",
  "description": "AppNexus Standard Eslint Config",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint . --ignore-pattern test-*",
    "lint-fix": "eslint . --ignore-pattern test-* --fix",
    "find-unused-rules": "eslint-find-rules -u -n",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check"
  },
  "pre-push": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/appnexus/eslint-config-appnexus"
  },
  "keywords": [
    "AppNexus",
    "config",
    "eslint",
    "eslintconfig",
    "javascript",
    "styleguide"
  ],
  "author": "Tim Santeford <tsanteford@appnexus.com>",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "dependencies": {
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0"
  },
  "bundleDependencies": [
    "prettier",
    "eslint-config-prettier",
    "eslint-plugin-prettier",
    "eslint-plugin-import",
    "eslint-plugin-promise"
  ],
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.9.0",
    "eslint-find-rules": "^3.3.1",
    "eslint-plugin-hapi": "^4.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-lodash": "^5.1.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "lodash": "^4.17.11",
    "pre-push": "^0.1.1",
    "prettier": "^1.15.3",
    "react": "^16.6.3"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
